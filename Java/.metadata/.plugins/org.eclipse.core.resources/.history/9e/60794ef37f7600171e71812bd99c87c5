package ex6_Ch5;

public class CountToEq {
	
	public int seeCount(int nr1, int nr2){
		int count = 0;
		//^ is XOR operation
		int nr = nr1^nr2;
		//count the number of ones in the sequence
		while(nr != 0) {
			if((nr & 1) == 1)
				count++;
			nr >>>= 1;
		}
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
