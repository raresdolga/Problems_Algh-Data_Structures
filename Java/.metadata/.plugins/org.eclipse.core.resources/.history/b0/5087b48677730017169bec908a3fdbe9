package ex12_ch4;

import java.util.HashMap;

public class NrOfSums {
	
	public int getTotalPaths(BSTree t, int targetSum){
		int totalPaths = 0;
		HashMap<Integer, Integer> apparitions = new HashMap<>();
		
		}
	public void getTotalPaths(Node root,HashMap<Integer,Integer> appar, int targetSum, int partialSum){
		int sum = partialSum + root.data;
		// get or default returns val if key exists or the default value otherwise
		int total = appar.getOrDefault(sum, 0);
		if(sum == targetSum)
			total = total + 1;
		
	}
	
