package ex6_Ch2;

import java.util.Scanner;

public class Palindrome { 
	// assume you do not have the size
	private static boolean checkPalindrome(Node<Integer> list){
		Node<Integer> current = list;
		boolean b = checkPalindrome(list.next);
		if(list != null && b){
			
			if(list.data.compareTo(current.data) != 0)
				return false;
			current = current.next;
		}
		return true;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner in = new Scanner(System.in);
		Node<Integer> head = new Node(in.nextInt());
		Node<Integer> aux = head;
		Node<Integer> now;
		while(in.hasNextInt()){
			now = new Node(in.nextInt());
			aux.next = now;
			aux = now;
		}
		now = head;
		System.out.println(checkPalindrome(now));
	}

}
