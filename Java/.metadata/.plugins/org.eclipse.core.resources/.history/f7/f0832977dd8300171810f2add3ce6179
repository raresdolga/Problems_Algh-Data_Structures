package ex9_ch7;

public class CircularArray<T extends Comparable<T>,Iterable> {
	private int size;
	private T[] arr;
	//head indicates to the first element
	private int head = 0;
	
	public CircularArray(int size){
		this.size = size;
		//cannot directly instantiate  from T type
		arr = (T[]) new Object[size];
	}
	
	public CircularArray(T[] arr){
		this.arr = arr;
		this.size = arr.length;
	}
	
	public int size(){
		return size;
	}
	
	
	//insert method
	
	public void rotate(){
		head += 1;
		if(head >= size)
			head %= size;
	}

}
