package ex4_ch5;
public class SmallestLargest {
	
	public int getBigger(int n) {
		int c = n;
		int c0 =0;
		int c1 = 0;
		while(((c & 1) == 0) && (c != 0)) {
			//c & 1 is checking last bit
			// count nr of 0 until the first one
			c = c >> 1;
		}
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
