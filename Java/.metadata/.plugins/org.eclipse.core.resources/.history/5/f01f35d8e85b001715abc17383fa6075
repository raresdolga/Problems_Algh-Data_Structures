package ex7_Ch2;

import java.util.HashMap;
import java.util.Iterator;
import java.util.Map;
import java.util.Scanner;

import javax.crypto.Mac;

public class Intersect {
	private static HashMap<Node<Integer>,Integer> checkIntersection(Node<Integer> head1, Node<Integer> head2){
		HashMap<Node<Integer>,Integer> freq = new  HashMap<Node<Integer>, Integer>();
		Node<Integer> p1 = head1;
		Node<Integer> p2 = head2;
		while(p1 != null){
			freq.put(p1, freq.get(p1) + 1);
			p1 = p1.next;
		}
		
		while(p2 != null){
			freq.put(p2, freq.get(p2) + 1);
			p2 = p2.next;
		}
		return freq;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner in = new Scanner(System.in);
		Node<Integer> head1 = new Node<Integer>(in.nextInt());
		Node<Integer> aux = head1;
		Node<Integer> t;
		
		while(in.hasNextInt()){
			t = new Node<Integer>(in.nextInt());
			aux.next = t;
			t = aux;
		}
		in = new Scanner(System.in);
		Node<Integer> head2 = new Node<Integer>(in.nextInt());
		aux = head2;

		while(in.hasNextInt()){
			t = new Node<Integer>(in.nextInt());
			aux.next = t;
			t = aux;
		}
		HashMap<Node<Integer>,Integer> test = checkIntersection( head1, head2);
		 Iterator it = test.entrySet().iterator();
		    while (it.hasNext()) {
		        Map.Entry pair = (Map.Entry)it.next();
		        System.out.println(pair.getKey() + " = " + pair.getValue());
		        it.remove(); // avoids a ConcurrentModificationException
		    }
	}

}
